programa
{
	
	funcao inicio()
	{
		inteiro i=1
		enquanto(i<=100){//while
			escreva(i+", ")
			i++ //i+1
		}
		escreva("\n\n Fim do Laço")
		para(inteiro j= 100;j>=0;j--){//for
			escreva(i+", ")
			i++ //i+1
			//para: Economiza linha
		}
		escreva("\n\n Fim do Laço")
	}
}
