programa
{
	
	funcao inicio()
	{	
		inteiro numero
		escreva("Digite um número limite para ser lido?\n")
		leia(numero)
		limpa()
		para(inteiro i=0;i<=numero;i++){
			escreva(i+" \n")
		}
	}
}
